{
  "workbench.colorTheme": "Default Dark+",
  "window.zoomLevel": 1,
  "files.eol": "\n",
  "editor.wordWrap": "on",
  "editor.formatOnSave": true,
  "remote.SSH.defaultForwardedPorts": [
    {
        "name": "Bencher API",
        "localPort": 61016,
        "remotePort": 61016
    },
    {
      "name": "Bencher UI",
      "localPort": 3000,
      "remotePort": 3000
    },
    {
      "name": "Vite HMR",
      "localPort": 3001,
      "remotePort": 3001
    },
  ],
  "rust-analyzer.cargo.allFeatures": false,
  "rust-analyzer.cargo.features": [],
  "rust-analyzer.imports.granularity.group": "crate",
  "prettier.configPath": "./services/ui/.prettierrc.json",
  "prettier.ignorePath": "./services/ui/.prettierignore",
  "prettier.prettierPath": "./services/ui/node_modules/prettier",
  "sqltools.connections": [
    {
      "previewLimit": 50,
      "driver": "SQLite",
      "name": "bencher",
      "database": "${workspaceFolder:bencher}/services/api/data/bencher.db"
    }
  ],
  "sqltools.useNodeRuntime": true,
  "cSpell.words": [
    "chrono",
    "clippy",
    "consts",
    "Deque",
    "dropshot",
    "ello",
    "epompeii",
    "fullwidth",
    "hmap",
    "Insertable",
    "jsonwebtoken",
    "litestream",
    "nanos",
    "oneshot",
    "perfs",
    "powi",
    "preheader",
    "Punct",
    "reqwest",
    "rqctx",
    "schemars",
    "signup",
    "udeps",
    "VITE"
  ]
}