{
  "workbench.colorTheme": "Default Dark+",
  "workbench.panel.defaultLocation": "right",
  "window.zoomLevel": 1,
  "files.eol": "\n",
  "editor.wordWrap": "on",
  "editor.formatOnSave": true,
  "rust-analyzer.cargo.allFeatures": false,
  "rust-analyzer.cargo.features": [],
  "rust-analyzer.imports.granularity.group": "crate",
  "prettier.configPath": "./services/ui/.prettierrc.json",
  "prettier.ignorePath": "./services/ui/.prettierignore",
  "prettier.prettierPath": "./services/ui/node_modules/prettier",
  "sqltools.connections": [
    {
      "previewLimit": 50,
      "driver": "SQLite",
      "name": "bencher",
      "database": "${workspaceFolder:bencher}/services/api/data/bencher.db"
    }
  ],
  "sqltools.useNodeRuntime": true,
  "cSpell.words": [
    "anychar",
    "bagge",
    "bencherdev",
    "bindgen",
    "CALENDLY",
    "chrono",
    "clippy",
    "consts",
    "Deque",
    "devel",
    "Dogfooding",
    "dropshot",
    "ello",
    "epompeii",
    "flyctl",
    "fullwidth",
    "hmap",
    "Insertable",
    "jsonwebtoken",
    "litestream",
    "Milli",
    "nanos",
    "netlifly",
    "oneshot",
    "perfs",
    "Pico",
    "powi",
    "preheader",
    "Punct",
    "reqwest",
    "rqctx",
    "rustfmt",
    "rustup",
    "schemars",
    "signup",
    "udeps",
    "VITE"
  ]
}