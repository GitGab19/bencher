{
  "openapi": "3.0.3",
  "info": {
    "title": "Bencher",
    "version": "0.1.0"
  },
  "paths": {
    "/v0/dba/migrate": {
      "put": {
        "operationId": "api_put_dba_migrate",
        "responses": {
          "200": {
            "description": "successful operation",
            "headers": {
              "Access-Control-Allow-Credentials": {
                "style": "simple",
                "schema": {
                  "nullable": true,
                  "type": "boolean"
                }
              },
              "Access-Control-Allow-Headers": {
                "style": "simple",
                "required": true,
                "schema": {
                  "type": "string"
                }
              },
              "Access-Control-Allow-Methods": {
                "style": "simple",
                "required": true,
                "schema": {
                  "type": "string"
                }
              },
              "Access-Control-Allow-Origin": {
                "style": "simple",
                "required": true,
                "schema": {
                  "type": "string"
                }
              }
            },
            "content": {
              "application/json": {
                "schema": {
                  "title": "String",
                  "type": "string"
                }
              }
            }
          },
          "4XX": {
            "$ref": "#/components/responses/Error"
          },
          "5XX": {
            "$ref": "#/components/responses/Error"
          }
        }
      }
    },
    "/v0/metrics": {
      "get": {
        "operationId": "api_get_metrics",
        "responses": {
          "200": {
            "description": "successful operation",
            "headers": {
              "Access-Control-Allow-Credentials": {
                "style": "simple",
                "schema": {
                  "nullable": true,
                  "type": "boolean"
                }
              },
              "Access-Control-Allow-Headers": {
                "style": "simple",
                "required": true,
                "schema": {
                  "type": "string"
                }
              },
              "Access-Control-Allow-Methods": {
                "style": "simple",
                "required": true,
                "schema": {
                  "type": "string"
                }
              },
              "Access-Control-Allow-Origin": {
                "style": "simple",
                "required": true,
                "schema": {
                  "type": "string"
                }
              }
            },
            "content": {
              "application/json": {
                "schema": {
                  "title": "Array_of_MetaMetrics",
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/MetaMetrics"
                  }
                }
              }
            }
          },
          "4XX": {
            "$ref": "#/components/responses/Error"
          },
          "5XX": {
            "$ref": "#/components/responses/Error"
          }
        }
      }
    },
    "/v0/reports": {
      "put": {
        "operationId": "api_put_reports",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Report"
              }
            }
          },
          "required": true
        },
        "responses": {
          "202": {
            "description": "successfully enqueued operation",
            "content": {
              "application/json": {
                "schema": {
                  "title": "Null",
                  "type": "string",
                  "enum": [
                    null
                  ]
                }
              }
            }
          },
          "4XX": {
            "$ref": "#/components/responses/Error"
          },
          "5XX": {
            "$ref": "#/components/responses/Error"
          }
        }
      }
    }
  },
  "components": {
    "responses": {
      "Error": {
        "description": "Error",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "#/components/schemas/Error"
            }
          }
        }
      }
    },
    "schemas": {
      "Duration": {
        "type": "object",
        "properties": {
          "nanos": {
            "type": "integer",
            "format": "uint32",
            "minimum": 0
          },
          "secs": {
            "type": "integer",
            "format": "uint64",
            "minimum": 0
          }
        },
        "required": [
          "nanos",
          "secs"
        ]
      },
      "Error": {
        "description": "Error information from a response.",
        "type": "object",
        "properties": {
          "error_code": {
            "type": "string"
          },
          "message": {
            "type": "string"
          },
          "request_id": {
            "type": "string"
          }
        },
        "required": [
          "message",
          "request_id"
        ]
      },
      "Latency": {
        "type": "object",
        "properties": {
          "duration": {
            "$ref": "#/components/schemas/Duration"
          },
          "variance": {
            "$ref": "#/components/schemas/Duration"
          }
        },
        "required": [
          "duration",
          "variance"
        ]
      },
      "MetaMetrics": {
        "type": "object",
        "properties": {
          "date_time": {
            "type": "string",
            "format": "date-time"
          },
          "hash": {
            "type": "integer",
            "format": "uint64",
            "minimum": 0
          },
          "id": {
            "type": "integer",
            "format": "uint",
            "minimum": 0
          },
          "length": {
            "type": "integer",
            "format": "uint",
            "minimum": 0
          },
          "metrics": {
            "type": "object",
            "additionalProperties": {
              "$ref": "#/components/schemas/Metric"
            }
          }
        },
        "required": [
          "date_time",
          "hash",
          "id",
          "length",
          "metrics"
        ]
      },
      "Metric": {
        "type": "object",
        "properties": {
          "latency": {
            "nullable": true,
            "allOf": [
              {
                "$ref": "#/components/schemas/Latency"
              }
            ]
          },
          "self_cpu": {
            "nullable": true,
            "type": "string",
            "enum": [
              null
            ]
          },
          "self_memory": {
            "nullable": true,
            "type": "string",
            "enum": [
              null
            ]
          },
          "throughput": {
            "nullable": true,
            "type": "string",
            "enum": [
              null
            ]
          },
          "total_cpu": {
            "nullable": true,
            "type": "string",
            "enum": [
              null
            ]
          },
          "total_memory": {
            "nullable": true,
            "type": "string",
            "enum": [
              null
            ]
          }
        }
      },
      "Report": {
        "type": "object",
        "properties": {
          "date_time": {
            "type": "string",
            "format": "date-time"
          },
          "email": {
            "type": "string"
          },
          "metrics": {
            "type": "object",
            "additionalProperties": {
              "$ref": "#/components/schemas/Metric"
            }
          },
          "project": {
            "nullable": true,
            "type": "string"
          },
          "testbed": {
            "$ref": "#/components/schemas/Testbed"
          },
          "token": {
            "type": "string"
          }
        },
        "required": [
          "date_time",
          "email",
          "metrics",
          "testbed",
          "token"
        ]
      },
      "Testbed": {
        "type": "object",
        "properties": {
          "arch": {
            "nullable": true,
            "type": "string"
          },
          "cpu": {
            "nullable": true,
            "type": "string"
          },
          "disk": {
            "nullable": true,
            "type": "string"
          },
          "name": {
            "nullable": true,
            "type": "string"
          },
          "os": {
            "nullable": true,
            "type": "string"
          },
          "os_version": {
            "nullable": true,
            "type": "string"
          },
          "ram": {
            "nullable": true,
            "type": "string"
          }
        }
      }
    }
  }
}