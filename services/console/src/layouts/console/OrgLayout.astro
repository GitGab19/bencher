---
import ConsoleLayout from "./ConsoleLayout.astro";
import OrgMenu from "../../components/console/menu/OrgMenu";
import type { Params } from "astro";

interface Props {
  params: Params;
  title: string;
}

const { params, title } = Astro.props;
---

<ConsoleLayout params={params} title={title}>
  <OrgMenu client:only slot="menu" params={params} />
  <slot />
</ConsoleLayout>
