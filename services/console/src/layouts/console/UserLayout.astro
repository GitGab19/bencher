---
import ConsoleLayout from "./ConsoleLayout.astro";
import UserMenu from "../../components/console/menu/UserMenu";
import type { Params } from "astro";

interface Props {
  params: Params;
  title: string;
}

const { params, title } = Astro.props;
---

<ConsoleLayout params={params} title={title}>
  <UserMenu client:only transition:persist="user_menu" slot="menu" />
  <slot />
</ConsoleLayout>
