---
import ConsoleLayout from "./ConsoleLayout.astro";
import ProjectMenu from "../../components/console/menu/ProjectMenu";
import type { Params } from "astro";

interface Props {
  params: Params;
  title: string;
}

const { params, title } = Astro.props;
---

<ConsoleLayout params={params} title={title}>
  <ProjectMenu
    client:only
    slot="menu"
    params={params}
  />
  <slot />
</ConsoleLayout>
