{
  "name": "bencher-ui",
  "type": "module",
  "version": "0.3.9",
  "scripts": {
    "typeshare": "typeshare ../.. --lang=typescript --output-file=./src/types/bencher.ts",
    "wasm": "../../scripts/build_wasm.sh",
    "setup": "npm run typeshare && npm run wasm",
    "dev": "npm run setup && astro dev",
    "start": "astro dev",
    "test": "npm run setup && vitest",
    "build": "export $(cat .env.runtime) && astro build",
    "netlify": "npm install && npx astro add netlify -y && npm run build",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/image": "^0.17.3",
    "@astrojs/mdx": "^0.19.7",
    "@astrojs/partytown": "^1.2.3",
    "@astrojs/sitemap": "^2.0.1",
    "@astrojs/solid-js": "^2.2.0",
    "@observablehq/plot": "^0.4.3",
    "@solid-primitives/resize-observer": "^2.0.6",
    "@solid-primitives/scheduled": "^1.4.1",
    "astro": "^2.10.5",
    "axios": "^1.4.0",
    "bulma": "^0.9.4",
    "bulma-divider": "^0.2.0",
    "bulma-pricingtable": "^0.2.0",
    "bulma-switch": "^2.0.4",
    "d3": "^7.6.1",
    "htl": "^0.3.1",
    "sharp": "^0.32.5",
    "solid-js": "^1.7.8",
    "swagger-ui-dist": "^4.15.5",
    "vite-plugin-wasm-pack": "^0.1.12"
  },
  "devDependencies": {
    "@types/d3": "^7.4.0",
    "remark-gfm": "^3.0.1",
    "rome": "^11.0.0",
    "sass": "^1.64.1",
    "vitest": "^0.34.2"
  }
}
